package Arrays;

public class Exercicio01Array {
	public static void main(String[] args) {
		
		   int[] numeros = {10, 20, 30, 40, 50};
		   
		   int somar = 0;
		   
		   for (int i = 0; i <numeros.length; i++) {
			   somar += numeros[i];
		   }
		   
		   System.out.println("Resultado da soma: " + somar);
		
		
	}

}
